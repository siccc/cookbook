{
  "name": "cookbook-app",
  "version": "0.0.0",
  "author": "Sarolta Nagy",
  "description": "Cookbook is an easy-to-use recipe app, the digital version of your treasured family recipe book.",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "vdev": "vercel dev --local-config ./vercel-dev.json",
    "build": "run-p type-check build-only prisma:generate",
    "preview": "vite preview --port 4173",
    "test": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "prisma:generate": "prisma generate"
  },
  "dependencies": {
    "@cloudinary/url-gen": "^1.16.1",
    "@codemirror/lang-markdown": "^6.0.3",
    "@formkit/auto-animate": "^0.8.1",
    "@libsql/client": "^0.6.0",
    "@prisma/adapter-libsql": "^5.14.0",
    "@types/dompurify": "^2.3.4",
    "@vueuse/core": "^9.5.0",
    "cloudinary": "^2.0.1",
    "codemirror": "^6.0.1",
    "cookie": "^0.5.0",
    "cross-fetch": "^3.1.5",
    "dompurify": "^2.5.4",
    "google-auth-library": "^8.8.0",
    "lodash": "^4.17.21",
    "vue": "3.4.29",
    "vue-codemirror": "^6.1.1",
    "vue-i18n": "9.13.1",
    "vue-query": "1.26.0",
    "vue-recaptcha": "^2.0.3",
    "vue-router": "4.3.3",
    "vue3-google-login": "^2.0.15",
    "vue3-markdown-it": "^1.0.10"
  },
  "devDependencies": {
    "@intlify/unplugin-vue-i18n": "4.0.0",
    "@prisma/client": "5.14.0",
    "@rushstack/eslint-patch": "1.2.0",
    "@tailwindcss/typography": "^0.5.7",
    "@types/cookie": "^0.5.1",
    "@types/jsdom": "20.0.0",
    "@types/lodash": "^4.14.187",
    "@types/node": "20.12.12",
    "@vercel/node": "3.1.5",
    "@vitejs/plugin-vue": "5.0.5",
    "@vue/eslint-config-typescript": "11.0.2",
    "@vue/test-utils": "2.1.0",
    "@vue/tsconfig": "0.1.3",
    "autoprefixer": "10.4.12",
    "eslint": "8.25.0",
    "eslint-plugin-vue": "9.6.0",
    "jsdom": "20.0.1",
    "node-mocks-http": "1.12.2",
    "npm-run-all": "4.1.5",
    "postcss": "^8.4.35",
    "prisma": "5.14.0",
    "tailwindcss": "3.1.8",
    "typescript": "4.8.4",
    "vercel": "34.2.0",
    "vite": "5.3.1",
    "vite-svg-loader": "^3.6.0",
    "vitest": "1.6.0",
    "vue-tsc": "1.0.3"
  },
  "overrides": {
    "@vercel/node": {
      "ts-node": "10.9.1",
      "typescript": "4.8.4"
    }
  }
