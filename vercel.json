{
  "rewrites": [
    {
      "source": "/api/recipes/:id?",
      "destination": "/api/api?resource=recipes&id=:id"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "s-maxage=1, stale-while-revalidate=59"
        }
      ]
    }
  ],
  "regions": [
    "fra1"
  ]
}